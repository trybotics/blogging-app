 (error) {\n      dispatch(loginFail());\n    });\n  };\n};\nexport var getNewUsers = function getNewUsers(skip) {\n  return function (dispatch) {\n    return axios.get(apiRoot + \"/api/user?limit=24&skip=\" + skip).then(function (response) {\n      if (response.data.length > 0) {\n        dispatch(setNewUsers(response.data));\n      }\n    }, function (error) {\n      console.log(error);\n    });\n  };\n};\nexport var getNewProjects = function getNewProjects(skip) {\n  return function (dispatch) {\n    return axios.get(apiRoot + \"/api/project?limit=24&skip=\" + skip).then(function (response) {\n      if (response.data.length > 0) {\n        dispatch(setNewProjects(response.data));\n      }\n    }, function (error) {\n      console.log(error);\n    });\n  };\n};\nexport var getNewInternships = function getNewInternships(page) {\n  return function (dispatch) {\n    return axios.get(apiRoot + \"/api/internship?page=\" + page).then(function (response) {\n      if (response.data.length > 0) {\n        dispatch(setNewInternships(response.data));\n      }\n    }, function (error) {\n      console.log(error);\n    });\n  };\n};\nexport var getNewBlogs = function getNewBlogs(skip) {\n  return function (dispatch) {\n    return axios.get(apiRoot + \"/api/blog?limit=24&skip=\" + skip).then(function (response) {\n      if (response.data.length > 0) {\n        dispatch(setNewBlogs(response.data));\n      }\n    }, function (error) {\n      console.log(error);\n    });\n  };\n};\nexport var showToast = function showToast(msg) {\n  return function (dispatch) {\n    return dispatch(setToast(msg));\n  };\n};\nexport var setCurrentUser = function setCurrentUser(user) {\n  user.isLogin = true;\n  return {\n    type: SET_CURRENT_USER,\n    user: user\n  };\n};\nexport var setUser = function setUser(user) {\n  user.isLogin = true;\n  return {\n    type: SET_USER,\n    user: user\n  };\n};\nexport var existingUser = function existingUser() {\n  return {\n    type: EXISTING_USER\n  };\n}